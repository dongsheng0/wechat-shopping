/* src/pages/xml2can/xml2can.wxss */
.page {
  /* padding-bottom: 400rpx; */
}

.container {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: blue;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.scroll-box {
  display: flex;
  flex-direction: row;
}

.spread_desc {
  font-size: 14px;
  color: rgba(85, 85, 85, 1);
  line-height: 22px;
}



.share-block {
  text-align: center;
}

.btn {
  display: inline-block;
  padding: 0px;
  font-size: 28rpx;
  cursor: pointer;
  text-align: center;
  color: #fff;
  background: linear-gradient(270deg, rgba(242, 111, 84, 1) 0%, rgba(255, 168, 99, 1) 100%);
  box-shadow: 0px 10px 15px 0px rgba(253, 159, 97, 0.24);
  border: 0;
}

.share-img {
  width: 240rpx;
  height: 90rpx;
  border-radius: 45rpx;
  line-height: 90rpx;

}

.share-img.img {
  background: linear-gradient(245deg, rgba(134, 237, 121, 1) 0%, rgba(200, 242, 100, 1) 100%);
  box-shadow: 0px 10px 15px 0px rgba(174, 239, 99, 0.37);
  margin-right: 38rpx;
}

.share-img image {
  display: inline-block;
  width: 26rpx;
  height: 24rpx;
  margin-right: 10rpx;
  margin-top: 4rpx;
}

.copy-content {
  padding: 72rpx;
}

.copy {
  float: right;
  display: block;
  width: 180rpx;
  height: 70rpx;
  font-size: 26rpx;
  border-radius: 35rpx;
  line-height: 70rpx;
}

.share_link {
  display: inline-block;
  border: 0;
  border-radius: 0;
  background: #fff;
  padding: 0;
  border-right: 0;
}

.share_link::after {
  border: 0;
}

.share_link image {
  width: 184rpx;
  height: 184rpx;
  display: block;
}

.share_link text {
  font-size: 26rpx;
  font-weight: 600;
  color: #727272;
  line-height: 36rpx;
}

.fix-bottom {
  /* position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2; */
  width: 100%;
  text-align: center;
  padding-bottom: 30rpx;
}

/* checkbox */
label {
  margin-left: 10px;
  position: relative;
}


.checkbox-pics {
  position: absolute;
  right: 13rpx;
  top: 20rpx;
  z-index: 10;
}

/*  重写 checkbox 样式  */
/* 未选中的 背景样式 */
checkbox .wx-checkbox-input {
  border-radius: 50%;
  /* 圆角 */
  width: 40rpx;
  /* 背景的宽 */
  height: 40rpx;
  /* 背景的高 */
}

/* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */
checkbox .wx-checkbox-input {
  border: none;
  width: 55rpx;
  height: 55rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 55rpx 55rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  width: 55rpx;
  height: 55rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 55rpx 55rpx;
}

/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  /* 圆角 */
  width: 40rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 40rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 40rpx;
  text-align: center;
  font-size: 30rpx;
  /* 对勾大小 30rpx */
  color: #fff;
  /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}